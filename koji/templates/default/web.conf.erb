[web]
SiteName = <%= node['koji_web.conf_KojiTheme'] %>
#KojiTheme = mytheme

# Key urls
KojiHubURL = <%= node['koji_web.conf_KojiHubURL'] %>
KojiFilesURL = <%= node['koji_web.conf_KojiFilesURL'] %>

# Kerberos authentication options
# WebPrincipal = koji/web@EXAMPLE.COM
# WebKeytab = /etc/httpd.keytab
# WebCCache = /var/tmp/kojiweb.ccache

# SSL authentication options
WebCert = <%= node['koji_web.conf_WebCert'] %>
ClientCA = <%= node['koji_web.conf_ClientCA'] %>
KojiHubCA = <%= node['koji_web.conf_KojiHubCA'] %>

LoginTimeout = <%= node['koji_web.conf_LoginTimeout'] %>

# This must be changed and uncommented before deployment
Secret = <%= node['koji_web.conf_Secret'] %>

LibPath = <%= node['koji_web.conf_LibPath'] %>
